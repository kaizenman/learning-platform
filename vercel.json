{
  "version": 2,
  "projects": {
      "frontend": {
          "builds": [{ "src": "package.json", "use": "@vercel/node" }],
          "routes": [{ "src": "/(.*).js", "dest": "frontend/build/$1.js" }]
      },
      "backend": {
          "builds": [{ "src": "server.js", "use": "@vercel/node" }],
          "routes": [{ "src": "/api/(.*)", "dest": "backend/server.js" }]
      }
  }
}
